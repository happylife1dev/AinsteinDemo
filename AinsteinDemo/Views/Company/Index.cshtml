@model AinsteinDemo.Models.Cube

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="display: flex; height:100%">
    @Html.Partial("~/Views/Shared/NavPanel.cshtml")
    <main class="main-container" role="main">
        <form>
            <div class="form-group searchBar">
                <input class="form-control" id="detailsCo" placeholder="Enter Ticker Symbol, e.g., AAPL">
                <button type="submit"><i class="fas fa-search"></i></button>
            </div>

                    <div class="tab-content" id="v-pills-tabContent">
                        <div class="row">
                            <div class="col-12 menu-tab">
                                <ul id="nav-tab-1" role="tablist">
                                    <li>
                                        <a class="active" id="nav-earnings-tab" data-toggle="tab" href="/Company/Details/AAPL/"
                                           aria-controls="nav-earnings" aria-selected="true" tabindex="0">
                                            Earnings
                                            View
                                        </a>
                                    </li>
                                    <li>
                                        <a id="nav-industry2-tab" data-toggle="tab" href="/ETF/Subindustry/AAPL"
                                           aria-controls="nav-industry2" aria-selected="false" tabindex="-1">
                                            Industry
                                            View
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </main>
    </div>
</div>

<script src="~/scripts/app.PortCube.js"></script>